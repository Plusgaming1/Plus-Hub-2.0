local function SFIRBT_fake_script() -- Tabs.LocalScript 
	local script = Instance.new('LocalScript', Tabs)

	local current = script.Parent.Parent.OpenTabs.Home

	for i, v in pairs(script.Parent:GetChildren()) do
		if v:IsA("TextButton") then
			v.MouseButton1Click:Connect(function()
				if current ~= nil then
					current.Visible = false
				end
				current = script.Parent.Parent.OpenTabs[v.Name]
				current.Visible = true
			end)
		end
	end
end
coroutine.wrap(SFIRBT_fake_script)()
