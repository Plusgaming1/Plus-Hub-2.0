--Scripts:

local function JSXIQG_fake_script() -- Welcome.LocalScript 
	local script = Instance.new('LocalScript', Welcome)

	script.Parent.Text = ("Welcome, ".. game.Players.LocalPlayer.Name.. ".")
end
coroutine.wrap(JSXIQG_fake_script)()
local function PUQXW_fake_script() -- UserImage.LocalScript 
	local script = Instance.new('LocalScript', UserImage)

	script.Parent.Image = game.Players:GetUserThumbnailAsync(game.Players.LocalPlayer.UserId,Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size352x352)
end
coroutine.wrap(PUQXW_fake_script)()
local function AHMAZ_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)

	script.Parent.ChildAdded:Connect(function()
		script.Parent.CanvasSize = script.Parent.CanvasSize + UDim2.new(0,0,0,25)
	end)
end
coroutine.wrap(AHMAZ_fake_script)()
local function JKWQ_fake_script() -- Morph.LocalScript 
	local script = Instance.new('LocalScript', Morph)

	repeat wait(1) until game.Loaded
	local open = false
	script.Parent.MouseButton1Click:Connect(function()
		if open then
			open = false
			script.Parent.ScrollingFrame.Visible = false
		else
			open = true
			script.Parent.ScrollingFrame.Visible = true
		end
	end)
	for i, v in pairs(game.Workspace:GetChildren()) do
		if v:IsA("Model") then
			if v:FindFirstChild("Humanoid") and v:FindFirstChild("Head") then
				if not game.Players:GetPlayerFromCharacter(v) then
					local button = script.Button:Clone()
					button.Parent = script.Parent.ScrollingFrame
					button.Text = v.Name
					button.Name = button.Text
					button.MouseButton1Click:Connect(function()
						firetouchinterest(game.Players.LocalPlayer.Character.Torso,v.Head,0)
						wait(0.1)
						firetouchinterest(game.Players.LocalPlayer.Character.Torso,v.Head,1)
					end)
					wait(0.1)
				end
			end
		end
	end
end
coroutine.wrap(JKWQ_fake_script)()
local function TBSEW_fake_script() -- MorphRemover.LocalScript 
	local script = Instance.new('LocalScript', MorphRemover)

	script.Parent.MouseButton1Click:Connect(function()
		firetouchinterest(game.Players.LocalPlayer.Character.Torso,game.Workspace["Morph remover"].Button,0)
		wait(0.1)
		firetouchinterest(game.Players.LocalPlayer.Character.Torso,game.Workspace["Morph remover"].Button,1)
	end)
end
coroutine.wrap(TBSEW_fake_script)()
local function ZHXNDDW_fake_script() -- Exit.LocalScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(ZHXNDDW_fake_script)()
local function WBAEX_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local e = false
	script.Parent.MouseButton1Click:Connect(function()
		if e then
			e = false
			script.Parent.Parent.Main.Visible = true
		else
			e = true
			script.Parent.Parent.Main.Visible = false
		end
	end)
end
coroutine.wrap(WBAEX_fake_script)()
local function SSWVSR_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	local function EnableDrag(GuiObject)
		local plr = game.Players.LocalPlayer
		local m = plr:GetMouse()
		if GuiObject:IsA("GuiButton") then
			local LastX = m.X
			local LastY = m.Y
			local Clicked = false
			GuiObject.MouseButton1Down:Connect(function()
				Clicked = true
				LastX = m.X
				LastY = m.Y
			end)
			m.Move:Connect(function()
				if Clicked then
					if (m.X ~= LastX) or (m.Y ~= LastY) then
						GuiObject.Position = UDim2.new(GuiObject.Position.X.Scale,GuiObject.Position.X.Offset - (LastX - m.X),GuiObject.Position.Y.Scale,GuiObject.Position.Y.Offset - (LastY - m.Y))
						LastX = m.X
						LastY = m.Y
					end
				end
			end)
			GuiObject.MouseButton1Up:Connect(function()
				Clicked = false
			end)
		elseif GuiObject:IsA("GuiBase") then
			local LastX = m.X
			local LastY = m.Y
			local Clicked = false
			m.Button1Down:Connect(function()
				local MinPos = GuiObject.AbsolutePosition
				local MaxPos = Vector2.new(GuiObject.AbsolutePosition.X + (GuiObject.AbsoluteSize.X), GuiObject.AbsolutePosition.Y + (GuiObject.AbsoluteSize.Y))
				if (m.X >= MinPos.X) and (m.Y >= MinPos.Y) and (m.X <= MaxPos.X) and (m.Y <= MaxPos.Y) then
					Clicked = true
					LastX = m.X
					LastY = m.Y
				end
			end)
			m.Move:Connect(function()
				if Clicked then
					if (m.X ~= LastX) or (m.Y ~= LastY) then
						GuiObject.Position = UDim2.new(GuiObject.Position.X.Scale,GuiObject.Position.X.Offset - (LastX - m.X),GuiObject.Position.Y.Scale,GuiObject.Position.Y.Offset - (LastY - m.Y))
						LastX = m.X
						LastY = m.Y
					end
				end
			end)
			m.Button1Up:Connect(function()
				Clicked = false
			end)
		end
	end

	EnableDrag(script.Parent)
end
coroutine.wrap(SSWVSR_fake_script)()

